  "scripts": {
    "start": "pbiviz start",
    "prebuild": "cd editor && npm run prodeditor",
    "audit": "yarn npm audit --environment production --severity moderate",
    "package": "pbiviz package",
    "dist": "pbiviz package",
    "pretest": "pbiviz package --resources --no-minify --no-pbiviz --no-plugin",
    "test": "karma start",
    "testdev": "cross-env NODE_ENV=test karma start",
    "cilint": "tslint -c ../tslint.json -p tsconfig.json",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "eslint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "fix": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "build:packages": "yarn nx run-many --target=build --projects=@visualbi/bifrost-editor,@visualbi/powerbi-common,@visualbi/bifrost-powerbi,@visualbi/bifrost-editor-rx,@visualbi/powerbi-editor",
    "build:mock-license": "lerna run build:mock-license-version --scope=@visualbi/bifrost-powerbi",
    "run:packages": "yarn nx run-many --target=dev --projects=@visualbi/powerbi-common,@visualbi/bifrost-powerbi",
    "prepare": "husky install",
    "clean": "lerna run clean --parallel",
    "version": "lerna version prerelease --no-push --no-changelog --force-publish=\"*\"",
    "version-patch": "lerna version patch --no-push --no-changelog --force-publish=\"*\"",
    "version-minor": "lerna version minor --no-push --no-changelog --force-publish=\"*\"",
    "publish-canary": "lerna publish from-package --canary --force-publish\"*\" --no-changelog --no-verify-access",
    "publish-packages": "lerna publish from-package --force-publish=\"*\" --no-git-tag-version --no-changelog --no-verify-access",
    "postinstall": "husky install"
  }
}